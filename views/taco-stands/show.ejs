<%- include('../partials/header.ejs') %>
<h3> Rating: 
  <% for (let i = 0; i < stand.rating; i++) { %>
    &#127790;
  <% } %>
</h3>

<ul>
  <li>
    City: <%= stand.city %>
  </li>
  <li>
    Price: <%= stand.price %>
  </li>
  <li>
    Hours: <%= stand.hours %>
  </li>
</ul>

<form action="/taco-stands/<%= stand._id %>?_method=DELETE" method="POST">
  <button type="submit">Delete Taco Stand</button>
</form>

